{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "node": "current"
      }}],
    ["@babel/preset-react", {"runtime": "automatic"}],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "i18next-extract",
      {
        "locales": ["en", "ru"],
        "keyAsDefaultValue": true,
        "discardOldKeys": false,
        "outputPath": "public/locales/{{locale}}/{{ns}}.json"
      }
    ]
  ]
}
