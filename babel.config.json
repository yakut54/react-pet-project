{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "i18next-extract",
      {
        "locales": ["en", "ru"],
        "keyAsDefaultValue": true,
        "discardOldKeys": false,
        "outputPath": "public/locales/{{locale}}/{{ns}}.json"
      }
    ]
  ]
}
